<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Admin.Mvc</title>
    <partial name="Partials/_PageStyles"></partial>
    <script src="~/theme/assets/vendor/js/helpers.js"></script>
    <script src="~/theme/assets/js/config.js"></script>
</head>
<body>

    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <div class="layout-page">
                <partial name="Partials/_Menu"></partial>
                <partial name="Partials/_Navbar"></partial>

                @RenderBody()


                <div class="layout-overlay layout-menu-toggle"></div>
            </div>
        </div>
    </div>
    <partial name="Partials/_PageScripts"></partial>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
